+		tsp_debug_err(&info->client->dev,
+			"%s TSP no accessible from Linux,"
+			" TUI is enabled!\n", __func__);
+		return -EIO;
+	}
+#endif
+
+	mutex_lock(&info->i2c_mutex);
+
+	xfer_msg[0].addr = info->client->addr;
+	xfer_msg[0].len = cnum;
+	xfer_msg[0].flags = 0;
+	xfer_msg[0].buf = reg;
+
+	xfer_msg[1].addr = info->client->addr;
+	xfer_msg[1].len = num;
+	xfer_msg[1].flags = I2C_M_RD;
+	xfer_msg[1].buf = buf;
