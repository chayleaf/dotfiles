+static void tfa98xx_irq_tfa2(struct tfa98xx *tfa98xx)
+{
+	pr_info("\n");
+
+	/*
+	 * mask interrupts
+	 * will be unmasked after handling interrupts in workqueue
+	 */
+	tfa_irq_mask(tfa98xx->tfa);
+	queue_delayed_work(tfa98xx->tfa98xx_wq, &tfa98xx->interrupt_work, 0);
+}
