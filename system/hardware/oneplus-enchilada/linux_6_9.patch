+static int fts_suspend(struct i2c_client *client, pm_message_t mesg)
+{
+	struct fts_ts_info *info = i2c_get_clientdata(client);
+
+	tsp_debug_info(&info->client->dev, "%s power state : %d\n",
+			__func__, info->fts_power_state);
