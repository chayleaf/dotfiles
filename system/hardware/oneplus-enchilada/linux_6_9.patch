+				}
+				/* pr_err("\n"); */
+				break;
+			case CMD_GETFWFILE:
+			case CMD_GETLIMITSFILE:
+				pr_info("Start To parse!\n");
+				for (j = 0; j < fileSize; j++) {
+					/* pr_err("%02X ", readData[j]); */
+					index += scnprintf(
