+int fts_get_noise_param_address(struct fts_ts_info *info)
+{
+	int rc = 0;
+	unsigned char regAdd[3];
+	unsigned char rData[3];
+	struct fts_noise_param *noise_param;
+	int i;
+
+	noise_param = (struct fts_noise_param *)&info->noise_param;
