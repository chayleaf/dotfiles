+	}
+
+	if (buf[0 + doffset])
+		ret = true;
+
+	tsp_debug_info(info->dev,
+		"%s: Final AFE [Data : %2X] AFE Ver "
+		"[Data : %2X]\n",
+		__func__,
+		buf[0 + doffset],
+		buf[1 + doffset]);
+
+	return ret;
+}
