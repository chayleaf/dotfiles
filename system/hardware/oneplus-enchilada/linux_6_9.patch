+	info->fts_write_reg(info, &cmd[0], 4);
+	info->cmd_state = CMD_STATUS_RUNNING;
+
+	while (info->cmd_state == CMD_STATUS_RUNNING) {
+		if (timeout++ > 30) {
+			info->cmd_state = CMD_STATUS_FAIL;
+			break;
+		}
+		fts_delay(10);
+	}
