+int tfa_is_cold(struct tfa_device *tfa)
+{
+	int value;
+
+	/*
+	 * check for cold boot status
+	 */
+	if (tfa->is_probus_device) {
+		if (tfa->ext_dsp > 0) {
+			if (tfa->ext_dsp == 2)
+				value = 0; // warm
+			else /* no dsp or cold */
+				value = 1; // cold
+		}
+		else {
+			value = (TFA_GET_BF(tfa, MANSCONF) == 0);
+		}
+	}
+	else {
+		value = TFA_GET_BF(tfa, ACS);
+	}
+
+	return value;
+}
